/* pages/particulars/particulars.wxss */

.body {
  background: #f2f2ef;
  height: 100%;
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
}

.content {
  clear: both;
}

.contentList {
  width: 100%;
}

.dividing {
  clear: both;
  height: 10px;
  width: 100%;
  background: #f2f2ef;
}

.labelLeft {
  float: left;
  background: url("https://maggie-public.oss-cn-beijing.aliyuncs.com/littleApps/label.png");
  background-size: cover;
  height: 30px;
  width: 58px;
  position: absolute;
  left: 0;
  top: 0;
}

.labelText {
  text-align: center;
  line-height: 30px;
  color: #fff;
  width: 48px;
  font-size: 13px;
}

.contentHead {
  height: 180px;
  position: relative;
}

.userBack {
  height: 100%;
  width: 100%;
}

.leabelRight {
  position: absolute;
  right: 10px;
  float: right;
  background: rgba(0, 0, 0, 0.50);
  border-radius: 10px;
  width: 90px;
  height: 28px;
  color: white;
  margin-top: 10px;
  text-align: center;
  line-height: 28px;
  font-size: 13px;
  margin-right: 10px;
}

.prize {
  color: #444;
  background: #fff;
  font-size: 17px;
  line-height: 50px;
  padding-left: 15px;
}

.lotteryData {
  background: #fff;
  border-bottom: 1px solid #e5e5e5;
  height: 30px;
  padding-left: 15px;
}

.lotteryData view {
  float: left;
  font-size: 13px;
  color: #939393;
}

.lotteryWay {
  margin-bottom: 10px;
  margin-left: 7px;
}

.lotteryData  .vertical {
  margin: 0 5px;
  height: 15px;
  overflow: hidden;
  color: #c3c3c3;
}

.trunSponsor {
  font-size: 14px;
  text-align: center;
  height: 40px;
  line-height: 40px;
  background: #f2f2ef;
  color: #787878;
}

.sponsor {
  background: #fff;
  clear: both;
  height: 60px;
  line-height: 60px;
}
.introduceTxt{
  width: 100%;
  color: #444;
}
.sponsor1 {
  margin-top: 10px;
}

.sponsorText {
  margin-left: 15px;
  font-size: 14px;
  color: #444;
  float: left;
}

.sponsorData {
  float: right;
}

.sponsorData view {
  font-size: 14px;
  color: #444;
  float: left;
}

.sponsorData view image {
  width: 25px;
  height: 25px;
  margin-bottom: -6px;
  margin-left: 10px;
  border-radius: 50px;
  margin-right: 15px;
}

.sponsorData view .rig {
  width: 6px;
  height: 12px;
  margin-bottom: 0;
}

.introduce view {
  font-size: 15px;
  color: #131313;
   /* margin-left: 15px; 
  margin-bottom: 23px;
   margin-right: 15px; */
}

.introduce .introduceHead {
  color: #444;
padding: 15px;
}

.foot {
  clear: both;
  padding-bottom: 100px;
  background: #f2f2ef;
  text-align: center;
}

.footPadding {
  padding-top: 43px;
  position: relative;
}

.participation {
  width: 96px;
  height: 96px;
  border: 10px solid #fa7635;
  margin: 0 auto;
  background: #fa8951;
  border-radius: 50%;
  font-size: 16px;
  color: #fff;
  text-align: center;
  line-height: 96px;
}

.participationY {
  border: none;
  width: 111px;
  height: 111px;
  line-height: 111px;
  position: relative;
  background: #fa7635;
}

.participationN {
  width: 111px;
  height: 111px;
  line-height: 111px;
  border: none;
  background: #aaa;
}

.num {
  padding-top: 40px;
  font-size: 13px;
  color: #848484;
  margin-bottom: 18px;
}

.photo image {
  width: 33px;
  height: 33px;
  margin: 0 2px;
}

.bottom {
  width: 100%;
  height: 49px;
  position: fixed;
  bottom: 0;
  left: 0;
}

.bottom view {
  width: 50%;
  font-size: 14px;
  color: #fff;
  float: left;
  height: 49px;
  line-height: 49px;
  text-align: center;
  background: #787878;
}

.bottom .bottomRight {
  background: #fff;
  color: #787878;
}

.bottomLeft {
  position: relative;
}

.userinfo {
  width: 111px;
  height: 111px;
  position: absolute;
  left: 50%;
  margin-left: -55px;
  z-index: 3;
   opacity: 0; 
}

.Submit {
  width: 111px;
  height: 111px;
  left: 50%;
  z-index: 2; opacity: 0; 
  top: 0;
  margin-top:45px;

  margin-left: -55px;
  position: absolute;
}

.isWon {
  width: 100%;
  height: 170px;
  background: #fff;
}

.noWonLogo {
  width: 54px;
  height: 54px;
  margin: 0 auto;
  padding-top: 32px;
  padding-bottom: 25px;
}

.noWonLogo image {
  width: 100%;
  height: 100%;
}

.noWonText {
  font-size: 17px;
  color: #a1a1a1;
  text-align: center;
}

.won {
  height: auto;
}

.wonLogo {
  width: 54px;
  height: 54px;
  margin: 0 auto;
  padding-top: 32px;
  padding-bottom: 25px;
}

.wonLogo image {
  width: 100%;
  height: 100%;
}

.wonText {
  font-size: 17px;
  color: #fa7635;
  text-align: center;
}

.wonGiftText {
  font-size: 15px;
  color: #444;
  text-align: center;
  margin-top: 30px;
  padding-bottom: 30px;
}

.addAddress {
  width: 127px;
  height: 43px;
  background: #efefef;
  border-radius: 10px;
  margin: 0 auto;
  font-size: 13px;
  text-align: center;
  line-height: 43px;
  color: #878787;
}

.noHasAddAddress {
  padding-bottom: 31px;
}

.hasAddress {
  width: 258px;
  height: 43px;
  background: #efefef;
  border-radius: 10px;
  margin: 0 auto;
  font-size: 13px;
  text-align: center;
  line-height: 43px;
  color: #878787;
}

.address {
  margin: 0 auto;
  font-size: 11px;
  width: 200px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #878787;
}

.modificationAddress {
  margin-top: 5px;
  font-size: 11px;
  color: #a1a1a1;
}

.hasAddAddress {
  padding-bottom: 12px;
}
