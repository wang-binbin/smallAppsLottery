/* pages/winUserInfo/winUserInfo.wxss */

.userUrl {
  width: 25px;
  height: 25px;
  float: left;
  margin: 15px 9px 0 17px;
}

.userUrl image {
  width: 100%;
  height: 100%;
  border-radius: 50px;
}

.userNickName {
  width: 240px;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 55px;
  height: 55px;
  font-size: 14px;
  color: #444;
  white-space: nowrap;
  float: left;
}

.winUser {
  margin-top: 10px;
  width: 100%;
  background: #fff;
}

.address {
  clear: both;
  height: 32px;
  width: 345px;
  background: #efefef;
  border-radius: 5px;
  margin: 0 auto;
  margin-top: 6px;
}
.noWrite{
  font-size: 12px;
color: #878787;line-height: 55px;
text-align: right;
margin-right: 15px;
}
.padding {
  clear: both;
  height: 1px;
  width: 100%;
}

.addressTxt {
  font-size: 11px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 320px;
  margin: 0 auto;
  color: #878787;
  line-height: 32px;
}
.paddingbottom{
  height: 20px;
  background: #fff;

}