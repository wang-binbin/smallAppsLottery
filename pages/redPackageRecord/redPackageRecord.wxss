/* pages/redPackageRecord/redPackageRecord.wxss */
page{
  background: #f2f2ef;
}
.swiper-tab {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 2;
  background: #fff;
  text-align: center;
  line-height: 80rpx;
  border-top: 1px solid #f2f2ef;
}
.more{
  text-align: center;
  line-height: 40px;
  color: #FA7635;
}
.swiper-tab-bott {
  position: absolute;
  height: 8rpx;
  border-radius: 50rpx;
  width: 190rpx;
  background: #fa7635;
  bottom: 0;
  left: 50%;
  margin-left: -48px;

}
.helpfixed{
  height: 50px;
  width: 100%;
  clear: both;
  background: #f2f2ef;
}
.swiper-tab-list {
  font-size: 32rpx;
  position: relative;
  display: inline-block;
  width: 50%;
  color: #7a7a7a;
}



.on {
  color: #fa7635;
  border-bottom-left-radius: 10%;
  border-bottom-right-radius: 10%;
}




.receive{
  background: #fff;
  height: 70px;
  padding: 0 15px;
  border-bottom: 1px solid  #D8D8D8;
}
.type{
  padding-top: 12px;
  padding-bottom: 8px;
}
.typeLeft{
  float: left;font-size: 16px;
color: #272727;
}
.typeRight{
  float: right;
  font-size: 16px;
  line-height: 1;
  padding-top: 2.5px;
  padding-bottom: 2.5px;
color: #272727;
}
.time{
  clear: both;
}
.timeLeft{
  float: left;font-size: 13px;
color: #A2A2A2;
}
.timeRight{
  float: right;
  font-size: 13px;
color: #A2A2A2;
}