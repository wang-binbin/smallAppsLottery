/* pages/papersParticulars/papersParticulars.wxss */
.banner{
  height: 215px;
  width: 100%;
  position: relative;
}
.bannerBack{
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
  width: 100%;
  height: 100%;
}
.headImg{
  height: 43px;
  width: 43px;
  margin: 0 auto;
  padding-top: 50px;
  margin-bottom: 12px;
}
.headImg image{
  width: 100%;
  height: 100%;
  border-radius:50%; 
}
.token{
  font-size: 15px;
color: #FFFFFF;
text-align:center;
}
.tokenNum{
    font-size: 22px;
color: #FFFFFF;
text-align:center;
margin: 12px auto;
margin-bottom: 6px;
}
.reserved_amount{
  font-size: 13px;
color: #FFFFFF;
opacity: 0.7;
text-align:center;
}
.padding{
  clear: both;
  background: #F2F2EF;
  height: 10px;
  width: 100%;
}
.content{
  margin: 0 15px;
}
.contentHead{
  font-size: 15px;
  line-height: 40px;
color: #444444;
}
.bottom{
    position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}
.bottom view{

  height: 50px;
  background: #FA7635 ;
  width: 50%;
  float: left;
  line-height: 50px;
  color: #fff;
  text-align: center;font-size: 15px;
}

.bottom .deposit{
background: #444444;

}
.contentTxt{
font-size: 15px;
color: #131313;
}
